@model IEnumerable<V2.Models.Version>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Commentaire)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Demo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Duree)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prix)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Album.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Chanson.Titre)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Commentaire)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Demo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Duree)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Prix)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Album.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Chanson.Titre)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.VersionID }) |
                @Html.ActionLink("Details", "Details", new { id = item.VersionID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.VersionID })
            </td>
        </tr>
    }

</table>
